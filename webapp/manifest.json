{
	"_version": "1.7.0",
	"sap.app": {
		"id": "simple-app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		],
		"fullWidth": true
	},
	"sap.ui5": {
		"handleValidation": true,
		"rootView": {
			"viewName": "simple-app.view.App",
			"type": "XML",
			"id": "AppView"
		},
		"dependencies": {
			"minUI5Version": "1.60.5",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.layout": {},
				"sap.collaboration": {},
				"sap.uxap": {},
				"sap.ui.table": {},
				"sap.ui.comp": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "simple-app.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "simple-app.view",
				"controlId": "AppView",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "index",
					"target": "SimpleTable",
					"targetAggregation": "pages"
				},
				{
					"pattern": "read",
					"name": "read",
					"target": "SimpleTable",
					"targetAggregation": "pages"
				},
				{
					"pattern": "edit",
					"name": "edit",
					"target": "SimpleTable",
					"targetAggregation": "pages"
				}
			],
			"targets": {
				"SimpleTable": {
					"viewName": "SimpleTable"
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}